{
  "name": "wrapper-sweetalert",
  "version": "1.0.0",
  "description": "Wrapper facilitador para uso de mensagens do SweetAlert2 em projetos JavaScript/Laravel",
  "main": "dist/sweetalert-wrapper.umd.js",
  "module": "dist/sweetalert-wrapper.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/sweetalert-wrapper.es.js",
      "require": "./dist/sweetalert-wrapper.umd.js"
    },
    "./dist/style.css": "./dist/sweetalert-wrapper.css"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "vite",
    "build": "npm run build:normal && npm run build:standalone",
    "build:normal": "vite build",
    "build:standalone": "vite build --config vite.config.standalone.js",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .js",
    "format": "prettier --write \"src/**/*.js\""
  },
  "keywords": [
    "sweetalert2",
    "sweetalert",
    "alert",
    "wrapper",
    "laravel",
    "validation",
    "toast",
    "notification",
    "i18n",
    "theme",
    "accessibility"
  ],
  "author": "Gabriel-stack",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Gabriel-stack/wrapper-sweetalert.git"
  },
  "bugs": {
    "url": "https://github.com/Gabriel-stack/wrapper-sweetalert/issues"
  },
  "homepage": "https://github.com/Gabriel-stack/wrapper-sweetalert#readme",
  "peerDependencies": {
    "sweetalert2": "^11.0.0"
  },
  "dependencies": {
    "sweetalert2": "^11.26.3"
  },
  "devDependencies": {
    "@vitest/ui": "^1.0.4",
    "eslint": "^8.55.0",
    "prettier": "^3.1.1",
    "terser": "^5.26.0",
    "vite": "^5.0.10",
    "vitest": "^1.0.4",
    "@vitest/coverage-v8": "^1.0.4"
  }
}
